{
  "name": "WerewolfBot",
  "description": "A discord bot written in discord.js which allows you to play The Werewolves of Miller's Hollows",
  "author": "BlackBearFTW#1331",
  "version": "1.0.0",
  "main": "./build/index.js",
  "license": "MIT",
  "scripts": {
    "start": "cd build && node index.js",
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec npm run dev:serve",
    "dev:serve": "tsc && npm start",
    "test": "ts-mocha test/**/*Test.ts test/**/**/*Test.ts --exit"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "mysql2": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@lambocreeper/mock-discord.js": "^1.0.9",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^15.12.2",
    "@types/sinon": "^10.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "eslint-config-codesupport": "^1.0.2",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "sinon": "^11.1.1",
    "typescript": "^4.3.2",
    "ts-mocha": "^8.0.0"
  }
}
